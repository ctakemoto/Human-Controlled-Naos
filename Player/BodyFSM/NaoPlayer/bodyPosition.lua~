module(..., package.seeall);

require('util')
require('Config')
require('gcm')


function entry()
  print(_NAME.." forced entry");
end

function update()
  changeState = gcm.get_body_state()
  if (changeState == _NAME) then
    return "position2position";
  end
  if (changeState == --NEED NAME!!!! how? ) then
    return "position2position";
  end
end

function exit()
end

